{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@types": ["src/types"],
      "@types/*": ["src/types/*"],
      "@routes": ["src/routes"],
      "@routes/*": ["src/routes/*"],
      "@utils": ["src/utils"],
      "@utils/*": ["src/utils/*"],
      "@apis": ["src/apis"],
      "@apis/*": ["src/apis/*"],
    }
  }
}
